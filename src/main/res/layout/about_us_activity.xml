<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:ignore="ContentDescription"/>

    <ImageButton
        android:id="@+id/back2"
        android:layout_width="38px"
        android:layout_height="60px"
        android:layout_marginTop="48px"
        android:layout_marginBottom="1812px"
        android:layout_marginStart="48px"
        android:layout_marginEnd="994px"
        android:background="@drawable/back_icon"
        tools:ignore="ContentDescription,PxUsage" />

    <ImageView
        android:id="@+id/error"
        android:layout_width="615px"
        android:layout_height="459px"
        android:layout_marginTop="570px"
        android:layout_marginBottom="891px"
        android:layout_marginStart="232px"
        android:layout_marginEnd="233px"
        android:background="@drawable/error404"
        tools:ignore="ContentDescription,PxUsage"/>

    <TextView
        android:id="@+id/t1"
        android:layout_width="match_parent"
        android:layout_height="240px"
        android:layout_marginTop="1000px"
        android:layout_marginBottom="680px"
        android:gravity="center"
        android:text="sorry"
        android:textColor="@color/black"
        android:textSize="16sp"
        tools:ignore="HardcodedText,PxUsage" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="match_parent"
        android:layout_height="240px"
        android:layout_marginTop="1070px"
        android:layout_marginBottom="610px"
        android:gravity="center"
        android:text="This section is not yet available"
        android:textColor="@color/black"
        android:textSize="16sp"
        tools:ignore="HardcodedText,PxUsage" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="match_parent"
        android:layout_height="240px"
        android:layout_marginTop="1140px"
        android:layout_marginBottom="540px"
        android:gravity="center"
        android:text="we working on it."
        android:textColor="@color/black"
        android:textSize="16sp"
        tools:ignore="HardcodedText,PxUsage" />

</androidx.drawerlayout.widget.DrawerLayout>
